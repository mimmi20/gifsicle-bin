{
	"name": "gifsicle",
	"version": "7.0.1",
	"description": "gifsicle wrapper that makes it seamlessly available as a local dependency",
	"license": "MIT",
	"repository": "imagemin/gifsicle-bin",
	"type": "module",
	"exports": "./index.js",
	"funding": {
		"url": "https://github.com/imagemin/gisicle-bin?sponsor=1"
	},
	"bin": {
		"gifsicle": "cli.js"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"postinstall": "node lib/install.js",
		"test": "xo && ava --timeout=60s"
	},
	"files": [
		"index.js",
		"cli.js",
		"lib",
		"vendor/source"
	],
	"keywords": [
		"imagemin",
		"gif",
		"img",
		"image",
		"compress",
		"minify",
		"optimize",
		"gifsicle"
	],
	"dependencies": {
		"@localnerve/bin-build": "^7.7.0",
		"@xhmikosr/bin-wrapper": "^13.0.5",
		"execa": "^9.6.0"
	},
	"devDependencies": {
		"ava": "^6.4.0",
		"@lesjoursfr/bin-check": "^7.1.9",
		"compare-size": "^3.0.0",
		"tempy": "^3.1.0",
		"xo": "^1.1.0"
	}
}
